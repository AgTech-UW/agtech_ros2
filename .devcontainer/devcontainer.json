{
  "name": "AgTech ROS 2 Humble",
  "image": "osrf/ros:humble-desktop-full",
  "privileged": true,
  "remoteUser": "root",
  "customizations": {
    "vscode": {
      "extensions": [
        "ms-iot.vscode-ros",
        "ms-python.python",
        "ms-vscode.cpptools"
      ]
    }
  },
  "containerEnv": {
    "QT_X11_NO_MITSHM": "1"
  },
  "runArgs": [
    "--network=host",
    "--cap-add=SYS_PTRACE",
    "--security-opt", "seccomp=unconfined",
    "--volume=/tmp/.X11-unix:/tmp/.X11-unix"
  ],
  "postCreateCommand": "echo 'source /opt/ros/humble/setup.bash' >> /root/.bashrc && mkdir -p /root/mystery_folder/level_1 && echo 'CODE: KERNEL_OF_TRUTH' > /root/mystery_folder/level_1/.hidden_payload.txt"
}